        <value>    try {&#13;
        var r = new sn_ws.RESTMessageV2('Lab 3 - Slack', 'post');&#13;
        // We want to get the current user so we call the getUser() function&#13;
        var myUserObject = gs.getUser();&#13;
        r.setStringParameter('username', myUserObject.getDisplayName()+'@servicenow');&#13;
        r.setStringParameter('color', '#ff0000');&#13;
        r.setStringParameter('state', current.state.getChoiceValue());&#13;
        r.setStringParameter('caller_name', current.caller_id.name);&#13;
        r.setStringParameter('short_description', current.short_description);&#13;
        r.setStringParameter('number', current.number);&#13;
        r.setStringParameter('caller_sys_id', current.caller_id);&#13;
        r.setStringParameter('priority', current.priority.getChoiceValue());&#13;
        r.setStringParameter('url', 'https://dev12672.service-now.com/');&#13;
        r.setStringParameter('icon_emoji', ':warning:');&#13;
        switch (current.cmdb_ci.sys_class_name.getChoiceValue()) {&#13;
            case 'Computer' : r.setStringParameter('cmdb_ci', ' :computer: ' + current.cmdb_ci.name); break;&#13;
            case 'Software' : r.setStringParameter('cmdb_ci', ' :floppy_disk: ' + current.cmdb_ci.name); break;&#13;
            case 'Printer' : r.setStringParameter('cmdb_ci', ' :printer: ' + current.cmdb_ci.name); break;&#13;
            case 'Service Offering' : r.setStringParameter('cmdb_ci', ' :briefcase: ' + current.cmdb_ci.name); break;&#13;
            case 'Business Service' : r.setStringParameter('cmdb_ci', ' :briefcase: ' + current.cmdb_ci.name); break;&#13;
            default : r.setStringParameter('cmdb_ci', ' :gear: ' + current.cmdb_ci.name);&#13;
        }&#13;
       &#13;
        var response = r.execute();&#13;
        var responseBody = response.getBody();&#13;
        var httpStatus = response.getStatusCode();&#13;
    }&#13;
    catch(ex) {&#13;
        var message = ex.getMessage();&#13;
}</value>
